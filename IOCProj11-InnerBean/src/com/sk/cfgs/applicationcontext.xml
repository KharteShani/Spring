<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	
	<bean id="custdetail" class="com.sk.beans.CustomerDetails">
		<constructor-arg>
			<bean class="com.sk.beans.BankAccount">
				<property name="bankname" value="sbihyd"/>
				<property name="holdername" value="raja"/>
				<property name="nriAccount" value="true"/> 
				<property name="balance" value="100000"/>
			</bean>
		</constructor-arg>
		<constructor-arg value='sbihyd'></constructor-arg>
	</bean>
</beans>
